                <li id="collexplink" class="explore closed"><a href="#">EXPLORE <i class="icon shanticon-directions"></i></a></li>
                <li class="dropdown lang highlight" id="block-locale-language">
<%                current_language = I18n.locale
                  language_metadata = LANGUAGES[current_language]
                  current_lang_title = language_metadata.nil? ? current_language.to_s : ts(language_metadata[:title], :locale => current_language) %>
                  <a href="#" class="dropdown-toggle" data-toggle="dropdown"><span><%= current_lang_title %></span><i class="icon shanticon-arrowselect"></i></a>
                  <ul class="dropdown-menu">
<%=                 render 'main/language_list' %>
                  </ul>
